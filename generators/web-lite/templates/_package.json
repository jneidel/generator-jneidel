{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "repository": "<%= username %>/<%= moduleName %>",
  "license": "MIT",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "<%= authorUrl %>"
  },
  "main": "app.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "concurrently \"npm run watch\" \"npm run build\" \"npm run testing\" --names \"ðŸ’»,ðŸ“¦,ðŸš€\" --prefix name",
    "watch": "nodemon app.js --ignore dist/ --ignore test/ --trace-sync-io",
    "build": "webpack -w",
    "test": "ava",
    "testing": "ava -w",
    "prod": "node -r dotenv/config app.js dotenv_config_path=vars.env"
  },
  "dependencies": {
    "body-parser": "latest",
    "connect-flash": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-session": "latest",
    "express-sessions": "latest",
    "mongoose": "latest",
    "mongoose-mongodb-errors": "latest",
    "normalize-scss": "latest",
    "pug": "latest"
  },
  "devDependencies": {
    "ava": "latest",
    "concurrently": "latest",
    "morgan": "latest",
    "nodemon": "latest",
    "setup-webpack": "latest"
  },
  "ava": {
    "files": [ "test.js" ]
  },
  "config": {
    "package-lock": false,
    "loglevel": "error"
  }
}
