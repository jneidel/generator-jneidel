{
  "manifest_version": 2,
  "name": "<%= moduleName %>",
  "description": "<%= description %>",
  "version": "0.0.0",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon.png",
      "48": "img/icon.png",
      "128": "img/icon.png"
    }
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}